        <PAGEOBJECT OwnPage="3" PTYPE="4" XPOS="138" YPOS="1979.5" WIDTH="514" HEIGHT="419" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="2" COLGAP="12" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" ISIZE="10" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 514 0 514 0 514 0 514 0 514 419 514 419 514 419 514 419 0 419 0 419 0 419 0 419 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 514 0 514 0 514 0 514 0 514 419 514 419 514 419 514 419 0 419 0 419 0 419 0 419 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="3" LAYER="0" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="Jednym z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="dwóch parków narodowych zlokalizowanych na terenie Lubelszczyzny jest Roztoczański PN. Obejmuje najcenniejsze przyrodniczo obszary Roztocza. Bardzo ciekawa tradycja i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="istnienie na jego obszarze Ośrodka Edukacyjno-Muzealnego dodatkowo podnoszą atrakcyjność tej instytucji. Do tego parku powinien więc obowiązkowo udać  się każdy miłośnik przyrody zamieszkujący Lubelszczyznę. "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="10" CH="HISTORIA POWSTANIA "/>
            <para ALIGN="3"/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="Tradycja ochrony przyrody na obszarze Roztocza sięga  końca XVI wieku, kiedy obszary te zostały włączone do założonej przez Kanclerza Wielkiego Koronnego, Jana Zamoyskiego, Ordynacji. Utworzono tu wówczas tzw. „zwierzyniec” – formę rezerwatu, w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="obrębie którego żyły "/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="jelenie, dziki, wilki, rysie, żbiki i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="tarpany.  Obejmował on powierzchnię obecnego obwodu ochronnego Bukowa Góra oraz część obwodu ochronnego Florianka. W"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="wyniku braku funduszy na utrzymywanie działalności pod koniec XVIII bądź na początku XIX w. „zwierzyniec” został zamknięty, a"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="zwierzęta w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="nim żyjące zostały wypuszczone do okolicznych lasów albo rozdane okolicznym mieszkańcom. Mimo to tradycje ochrony przyrody były jednak ciągle kultywowane na tym obszarze. Duże zainteresowanie badaczy terenem Roztocza, spowodowane mnogością unikalnych gatunków roślin i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="zwierząt przyczyniło się do powołania w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="1974 r. Roztoczańskiego Parku Narodowego, obejmującego 8482"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="ha. "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="10" FCOLOR="Registration" CH="ŚWIAT ROŚLIN"/>
            <para ALIGN="3"/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="Flora roślin Roztoczańskiego Parku Narodowego liczy ponad 1000 gatunków. Są wśród nich rośliny górskie (np. tojad dzióbaty, czosnek siatkowaty), borealne (zimoziół północny, widłak wroniec, pomocnik baldaszkowaty), atlantyckie (rosiczka pośrednia, sporek wiosenny, sit sztywny) oraz pontyjskie (przetacznik ząbkowany, szczodrzeniec ruski). Wiele z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="nich to gatunki reliktowe – pozostałości okresów zlodowaceń i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="ocieplenia klimatu. Ścisłej ochronie gatunkowej podlega 45 gatunków, a"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="65 uznano za rzadkie. Szczególną wagę przywiązuje się w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="parku do ochrony takich roślin jak"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="m.in. śnieżyczka przebiśnieg, rosiczka długolistna i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="okrągłolistna, lilia złotogłów, rojnik złotogłów, wszystkie (15) gatunków występujących tu storczyków, na czele z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="obuwikiem pospolitym (mającym w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="uroczysku Maziarki jedno z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="największych stanowisk w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="Polsce). "/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="10" CH="ŚWIAT ZWIERZĄT"/>
            <para ALIGN="3"/>
            <tab FONTSIZE="10"/>
            <ITEXT FONTSIZE="10" CH="W"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="faunie RPN przeważają gatunki leśne. Spośród ssaków liczne są jeleniowate: jelenie, sarny, daniele i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="łosie, oraz drapieżniki, w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="tym wilki, wydry i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="rysie. Dość często występują gryzonie: bóbr, popielica, orzesznica i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="rzadko spotykana w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="kraju koszatka. Latające ssaki – nietoperze – reprezentują 15 gatunków z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="21 mieszkających na terenie Polski. Do najcenniejszych gatunków awifauny parku zalicza się: bociana czarnego, orlika krzykliwego, trzmielojada, derkacza, jarząbka, brodźca samotnego, sieweczkę rzeczną, dzięcioła białogrzbietego i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="gwałtownie zanikającą kraskę. W"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="Roztoczańskim Parku Narodowym  zamieszkuje blisko 300 gatunków zwierząt podlegających ochronie , w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="tym wiele bezkręgowców, jak np. nadobnica alpejska czy jelonek rogacz. RPN jest również jedyną w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="Polsce ostoją konika polskiego, która to rasa postała w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="wyniku skrzyżowania wymarłego już tarpana z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="końmi z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10" CH="okolicznych zagród chłopskich."/>
            <para ALIGN="3"/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="10" CH="Agata T."/>
            <trail ALIGN="2"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
