            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" FCOLOR="Red" FSHADE="100" CH="UWAGA!!"/>
            <para ALIGN="1"/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="WIADOMOŚĆ Z"/>
            <nbspace/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="OSTATNIEJ CHWILI - GIMNAZJUM NR 1 W"/>
            <nbspace/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="NIEDRZWICY DUŻEJ PRZYCZYNIŁO SIĘ DO ROWOJU EPIDEMII RECYKLINGU I"/>
            <nbspace/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="DBANIA O"/>
            <nbspace/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="ŚRODOWISKO!"/>
            <para ALIGN="1"/>
            <para ALIGN="1"/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="ZBIÓRKA NAKRĘTEK - PRZYCZYNA"/>
            <para ALIGN="1"/>
            <tab FONTSIZE="8"/>
            <ITEXT FONTSIZE="8" CH="Właściwie można powiedzieć, że zbiórka nakrętek w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="naszym gimnazjum była od zawsze. Wszystko zaczęło się od ogłoszenia niewinnej akcji, którą każdy mógł wspomóc wrzucając plastikowe zakrętki do postawionego na szkolnym korytarzu wielkiego pudła. I"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="tak właśnie wyglądał początek, alboteż przyczyna epidemii recyklingu i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nie tylko... Chodzą słuchy, że za pierwszym razem zebrano tak dużą ilość nakrętek, że niektórym nauczycielom do tej pory śnią się po nocach ich przerażająco wielkie stosy, które następnie są transportowane ogromną ciężarówką do Gdańska na rzecz chorego chłopca. Dzięki tej akcji nie tylko zadbaliśmy o"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nasze środowisko, ale również wspomogliśmy drugiego człowieka. Na podstawie własnego doświadczenia mogę stwierdzić, że to właśnie działanie najbardziej przyczyniło się do otwarcia młodych ludzi na potrzeby otaczającego ich środowiska. Pokazało, że wystarczy naprawdę niewiele, aby dokonać czegoś o"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="gigantycznym znaczeniu dla siebie, świata i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="innych ludzi. Dlatego poruszeni i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="zapaleni do pracy gimnazjaliści zaczęli sami proponować, realizować i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="rozpowszechniać akcje przynoszące wspaniałe skutki. Pozbierali porozrzucane wokoło szkoły i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="najbliższej okolicy puszki, które potem zamienili na piłki do grania. Nauczyli się rozsądnie gospodarować wolnym czasem, tak aby zrobić coś pożytecznego. Celebrując &quot;Dzień Ziemi&quot; posadzili wokoło gimnazjum młode drzewka, krzewy i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="kwiatki oraz wyplewili ogródki. I"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="ten właśnie sposób epidemia recyklingu przeobraziła się w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="epidemię dbania o"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="własne środowisko. Pojemniki na nakrętki i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="puszki stały się codziennością w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="naszym gimnazjum i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nikogo nie dziwi ich obecność. Każdy gimnazjalista wie co zrobić z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="„niepotrzebnymi” nakrętkami czy puszkami. "/>
            <para ALIGN="3"/>
            <ITEXT FONTSIZE="8" CH="Patrycja"/>
            <para ALIGN="2"/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="A"/>
            <nbspace/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="OTO NAKRĘTKOWY ZAWRÓT GŁOWY "/>
            <trail ALIGN="1"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
        <PAGEOBJECT OwnPage="5" PTYPE="4" XPOS="140.28" YPOS="3269.67" WIDTH="512.72" HEIGHT="196.83" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="2" COLGAP="12" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" ISIZE="8" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 512.72 0 512.72 0 512.72 0 512.72 0 512.72 196.83 512.72 196.83 512.72 196.83 512.72 196.83 0 196.83 0 196.83 0 196.83 0 196.83 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 512.72 0 512.72 0 512.72 0 512.72 0 512.72 196.83 512.72 196.83 512.72 196.83 512.72 196.83 0 196.83 0 196.83 0 196.83 0 196.83 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="0" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="KONKURS TESCO - EWOLUCJA"/>
            <para ALIGN="1"/>
            <tab FONTSIZE="8"/>
            <ITEXT FONTSIZE="8" CH="Od dwóch lat nasza szkoła uczestniczy w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="konkursie filmowym Tesco dla szkół, który w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="tym roku przebiegał pod hasłem „Talent do niemarnowania”. Naszym zadaniem było nakręcenie tematycznego video, które następnie zostało zamieszczone na stronie internetowej http://www.tescodlaszkol.pl/. Cel tego przedsięwzięcia zakładał propagowanie i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="szerzenie zdrowego stylu życia oraz zachęcenie zarówno młodzieży jak i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="dorosłych do racjonalnego odżywiania oraz niemarnowania żywności. Każdy"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH=" uczeń „miał obowiązek” systematycznego głosowania na filmik naszego gimnazjum. Do tej pory można go zobaczyć korzystając z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="linku:"/>
            <para/>
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="16" FEATURES="inherit underline" CH="www.goo.gl/5Xaorz"/>
            <para ALIGN="1"/>
            <tab FONTSIZE="8"/>
            <ITEXT FONTSIZE="8" CH="Więc jeśli się komuś bardzo nudzi i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nie ma pojęcia co ze sobą zrobić, albo ma niezmierną ochotę się zwyczajnie pośmiać już wie gdzie może szukać atrakcji. Z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="jednej strony poważni,"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH=" niosący z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="sobą głębokie przesłanie młodzi ludzie, z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="drugiej kipiący pozytywną energią i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="chęcią działania gimnazjaliści. Czy to nie jest wybuchowa mieszanka? - Na pewno! Każdy śmiałek, który zdobędzie się na odwagę i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="obejrzy film oparty na autentycznych faktach pt. &quot;Rozsądne głodomory&quot; narażony jest na stanie się ofiarą epidemii zdrowego odżywiania. Jak podają najnowsze informacje stopniowo opanowuje ona cały świat. Efekty jej działania zauważalne są przede wszystkim w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="mojej szkole. Otóż ostatnio w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="Gimnazjum nr"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="1 w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="Niedrzwicy Dużej zaobserwowano bardzo dziwne zjawisko. Uczniowie zaczęli segregować śmieci, zjadać całe porcje obiadu, przestali kupować pepsi, a"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="zamiast tego coraz częściej można ich spotkać na korytarzu pijących wodę mineralną. Mało tego przejęci ochroną środowiska zgniatają niepotrzebne butelki i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="wyrzucają do odpowiednio oznaczonych kontenerów. I"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="to nie jest żart proszę państwa! Ta historia wydarzyła się na prawdę. Jeśli nie wierzycie, i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nie brak wam odwagi przyjedźcie tutaj, bo to co tu spotkacie może na dobre odmienić wasze życie i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="postrzeganie świata. Zapewniam, że wrócicie do domu jako zupełnie inni ludzie. Zaufajcie mi - wiem coś o"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="tym. Sama przeżyłam to doświadczenie i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="przeżywam ciągle gdy tylko jestem w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="szkole .... i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="nie tylko..."/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH=" To wspaniałe widzieć jak dzięki naszym staraniom świat staje się piękniejszy - spróbuj i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="TY! Zapewniam, że nie pożałujesz. A"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="może pewnego dnia tak jak my zarazisz kogoś swoją pasją pomagania światu być czystszym i"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="przyjemniejszym?"/>
            <para/>
            <ITEXT FONTSIZE="8" CH="Patrycja"/>
            <trail ALIGN="2"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
        <PAGEOBJECT OwnPage="6" PTYPE="4" XPOS="736.5" YPOS="2968" WIDTH="513" HEIGHT="81" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="2" COLGAP="12" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" ISIZE="8" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 513 0 513 0 513 0 513 0 513 81 513 81 513 81 513 81 0 81 0 81 0 81 0 81 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 513 0 513 0 513 0 513 0 513 81 513 81 513 81 513 81 0 81 0 81 0 81 0 81 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="0" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONT="Liberation Serif Bold" FONTSIZE="9" CH="ZBIÓRKA TELEFONÓW - KULMINACJA"/>
            <para ALIGN="1"/>
            <tab FONTSIZE="8"/>
            <ITEXT FONTSIZE="8" CH="Od niedawna w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="naszym gimnazjum Samorząd Uczniowski organizuje zbiórkę starych, używanych, niekoniecznie działających telefonów. Każdy może wspomóc akcję oddając niepotrzebne komórki, które następnie zostaną przekazane do profesjonalnego recyklingu. Muszę przyznać, że to przedsięwzięcie cieszy się ogromnym powodzeniem. Mało tego możliwość wygrania aparatu fotograficznego firmy Canon dodatkowo zachęca uczniów do wsparcia akcji. Osobiście nie pamiętam dnia, aby do drzwi sali, w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="której miałam lekcje nie zapukała trzyosobowa delegacja zbierająca telefony. Aż trzyosobowa? - tak duża? Nic dziwnego. Jeden uczeń nie da rady utrzymać wielkiego pudła - potrzeba co najmniej dwóch! Nie ma klasy, która nie oddałaby chociaż paru telefonów. Tak się wszyscy w"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="to wkręcili, że z"/>
            <nbspace/>
            <ITEXT FONTSIZE="8" CH="niecierpliwością czekają, aż znajome pudło &quot;dumnie wkroczy&quot; do sali."/>
            <para/>
            <ITEXT FONTSIZE="8" CH="Patrycja"/>
            <trail ALIGN="2"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
