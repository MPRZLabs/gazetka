            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="W"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="co się ubrać zimą?  Oczywiście powinno być modnie i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="wygodnie… A"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="jak konkretnie? Podpowiemy, czego nie powinno zabraknąć w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="Twojej szafie w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="te zimne miesiące."/>
            <para ALIGN="3"/>
            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="Zacznijmy od nakrycia głowy. W"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="tym sezonie modne będą czapki typu KRASNAL, dozwolone są wszelkie kolory, więc wybór należy do Ciebie!"/>
            <para ALIGN="3"/>
            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="Następnie niższa partia ciała, czyli okrycie wierzchnie. Hitem sezonu będą kurtki - tzw. PARKA w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="kolorze khaki lub moro. "/>
            <para ALIGN="3"/>
            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="Kolejnym elementem damskiej garderoby, szalenie modnym i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="pożądanym, będą getry w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="norweskie wzory albo bardziej tradycyjne spodnie jeansowe."/>
            <para ALIGN="3"/>
            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="Na koniec jedna z"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="najważniejszej części Twojej garderoby, czyli buty. Zimą najlepiej będą się sprawdzały ŚNIEGOWCE, ich kolor również zależy od Twoich upodobań."/>
            <para ALIGN="3"/>
            <tab FONTSIZE="9.5"/>
            <ITEXT FONTSIZE="9.5" CH="Gdy zastosujesz się do naszych rad i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="uzupełnisz swoją szafę o"/>
            <nbspace/>
            <ITEXT FONTSIZE="9.5" CH="kilka drobiazgów, możesz być pewna, że będziesz najmodniej ubraną dziewczyną tego sezonu."/>
            <trail ALIGN="3"/>
