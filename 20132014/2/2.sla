            <ITEXT FONT="Liberation Sans Bold" FONTSIZE="9" CH="75. Głodowe Igrzyska czas zacząć! 22 listopada bieżącego roku do kin weszła ekranizacja kontynuacji cieszącej się dużą sławą na całym świecie powieści „Igrzyska śmierci” Suzanne Collins. „W"/>
            <nbspace/>
            <ITEXT FONT="Liberation Sans Bold" FONTSIZE="9" CH="Pierścieniu Ognia” stanowi dosyć wierną adaptację opowieści, podkreślając najważniejsze aspekty historii  i"/>
            <nbspace/>
            <ITEXT FONT="Liberation Sans Bold" FONTSIZE="9" CH="przesłanie autorki."/>
            <para/>
            <tab FONTSIZE="9"/>
            <ITEXT FONTSIZE="9" CH="Katniss Everdeen i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="Peeta Mellark, zwycięzcy ostatnich Głodowych Igrzysk, biorą udział w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="dorocznym Tournée Zwycięzców. Ich zadaniem jest publiczne wystąpienie w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="każdym z"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="Dwunastu Dystryktów i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="oddanie hołdu poległym trybutom. Prowokacyjne zachowanie dziewczyny stanowi dla uciśnionego przez kapitolińskie władze ludu wyraz buntu. Wkrótce potem cały kraj zalewa powstanie. W"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="odwecie prezydent Snow w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="celu uczczenia jubileuszowych igrzysk angażuje w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="śmiertelną rozgrywkę zwycięzców"/>
            <breakline/>
            <breakline/>
            <breakline/>
            <breakline/>
            <ITEXT FONTSIZE="9" CH="z"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="ubiegłych lat. Dla Katniss i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="Peety oznacza to powrót na arenę… "/>
            <para/>
            <tab FONTSIZE="9"/>
            <ITEXT FONTSIZE="9" CH="Pełna rozmachu ekranizacja pod względem wyrazu nie odbiega od filmowej adaptacji pierwszej części trylogii. Świetna gra aktorska i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="nowoczesne rozwiązania techniczne utrzymują widza przez cały czas w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="pełnym niecierpliwego wyczekiwania napięciu. Dodatkowym atutem jest odpowiednie zobrazowanie problematyki moralności, z"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="jaką styka się Katniss Everdeen (w"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="tej roli Jennifer Lawrence). Wszystkie elementy dzieła tworzą spójną całość, zwieńczoną puentą o"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="lekkim zabarwieniu filozoficznym.  "/>
            <para/>
            <tab FONTSIZE="9"/>
            <ITEXT FONTSIZE="9" CH="Podsumowując, mogę powiedzieć, że ten film jest idealnym sposobem spędzenia czasu dla amatorów dobrej gry aktorskiej i"/>
            <nbspace/>
            <ITEXT FONTSIZE="9" CH="dynamicznej akcji."/>
