            <ITEXT FONT="FreeSerif Bold Italic" FONTSIZE="10.5" CH="Każdy z"/>
            <nbspace/>
            <ITEXT FONT="FreeSerif Bold Italic" FONTSIZE="10.5" CH="nas czeka całe swoje życie "/>
            <breakline/>
            <ITEXT FONT="FreeSerif Bold Italic" FONTSIZE="10.5" CH="na taki piękny i"/>
            <nbspace/>
            <ITEXT FONT="FreeSerif Bold Italic" FONTSIZE="10.5" CH="niezwykły wieczór…"/>
            <para ALIGN="2"/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="18 stycznia 2014 roku o"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="godz.16:00 w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="Gimnazjum Nr 1 im. Marszałka Józefa Piłsudskiego w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="Niedrzwicy Dużej rozpoczął się uroczysty Bal Gimnazjalny. "/>
            <para/>
            <ITEXT FONTSIZE="10.5" CH="Zgromadzeni goście, nauczyciele, rodzice, rodzeństwo, babcie  i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="dziadkowie, szepty w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="kuluarach, ostatnie poprawki fryzur…Wreszcie pierwsze tony poloneza, nasze pierwsze kroki tego tradycyjnego tańca…"/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Zgromadzeni z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="podziwem przypatrywali się  wyszukanym kreacjom, bo tego dnia wszyscy bez wyjątku prezentowali się wspaniale. Błyskające flesze miały uchwycić nie tylko głównych bohaterów spotkania, ale także fantastyczną, dopracowaną dekorację sali. Bez wątpienia widok delikatnych kwiatów wiśni, efektownej pagody, kolorowych wachlarzy, lampionów, rozświetlonego bonsai, starego japońskiego miasta cieni i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="innych elementów zapierał  dech w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="piersiach. Widok tak niesamowity, że zdawało się, iż  tej nocy przenieśliśmy się do bajecznej Japonii. "/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Uroczystość oficjalnie otworzyły słowa pani dyrektor Justyny Polaczek i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="pana wójta Adama Kuny. Następnie nadeszła pora na podziękowania skierowane do rodziców, którym dedykowana była piosenka „Cudownych rodziców mam” i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="wspólny taniec. Ledwie zasiedliśmy do pięknie przystrojone stołów, a"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="już porwały nas rytmy taneczne. Z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="całą pewnością nikt nie ma prawa narzekać na nudę, bo co chwilę „serwowano” artystyczne niespodzianki: wtargnięcie na salę policjanta, który przedstawił prawdziwe, filmowe dowody na pomysłowość gimnazjalistów, atak terrorystyczny z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="powietrza i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="lądu zakończony piosenką, mini-spektaklem i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="poszukiwaniem kluczy. Tu dodać należy, że zapadną w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="naszej pamięci życiowe role (i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="rekwizyty) Wojciecha jako pana Wacława oraz Michała jako świętego Piotra."/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Kulminacyjnym momentem niedrzwickich balów jest przyznanie statuetek dla tych, którzy przez trzy lata edukacji wsławili się szczególnymi osiągnięciami w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="różnych dziedzinach. Tym razem wręczanie podzielono na dwie części. Na szczególną uwagę zasługuje zwieńczenie pierwszej części - niesamowita jazda ekstremalna na rowerze w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="wykonaniu naszego kolegi, sławnego już w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="Polsce Pawła Pasierbowicza. Część drugą zakończyła kategoria „Niezastąpiony Roku” i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="tu – ku zdumieniu gimnazjalistów i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="publiczności – wszyscy zostali nominowani! Był to bardzo wzruszający moment. Szczerze także trzeba przyznać, iż konferansjerzy wykazali się wielką swobodą  i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="poczuciem humoru - publiczność szalała ze śmiechu."/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Wreszcie nie można nie wspomnieć o"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="kruchych ciasteczkach  z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="zatopionymi wewnątrz oryginalnymi japońskimi wróżbami – otrzymali je wszyscy zgromadzeni. Ciekawe, czy się sprawdzą!"/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Poza tym nie zapomnimy, że wraz z"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="wychowawcami, nauczycielami i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="panią dyrektor tańczyliśmy wspólnego &quot;wężyka&quot;, pociąg, a"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="oprócz tego tańce - odbijańce. Ale się działo! Parkiet był rozgrzany do czerwoności. "/>
            <para/>
            <tab FONTSIZE="10.5"/>
            <ITEXT FONTSIZE="10.5" CH="Nieco po północy niebo rozświetliły papierowe lampiony - „światełko do nieba”. I"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="był to ostatni akcent tego pełnego wrażeń wieczoru, którego uroda tkwi w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="naszej młodości, wspólnej pracy i"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="wspólnej zabawie. "/>
            <para/>
            <para/>
            <ITEXT FONTSIZE="10.5" CH="Wszystko, co piękne, szybko się kończy, niech więc w"/>
            <nbspace/>
            <ITEXT FONTSIZE="10.5" CH="sercu pozostaną wspomnienia..."/>
            <para/>
            <para/>
            <ITEXT FONT="Mistral Regular" FONTSIZE="11.5" CH="Weronika"/>
            <trail ALIGN="2"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
